{
    "multipart": [
        {"when": { "type": "oak", "rotated": "false" }, "apply": { "model": "ropebridge:bridge_block_2_oak" }},
        {"when": { "type": "oak", "rotated": "true" }, "apply": { "model": "ropebridge:bridge_block_2_oak", "y": 90 }},
        {"when": { "type": "spruce", "rotated": "false" }, "apply": { "model": "ropebridge:bridge_block_2_spruce" }},
        {"when": { "type": "spruce", "rotated": "true" }, "apply": { "model": "ropebridge:bridge_block_2_spruce", "y": 90 }},
        {"when": { "type": "birch", "rotated": "false" }, "apply": { "model": "ropebridge:bridge_block_2_birch" }},
        {"when": { "type": "birch", "rotated": "true" }, "apply": { "model": "ropebridge:bridge_block_2_birch", "y": 90 }},
        {"when": { "type": "jungle", "rotated": "false" }, "apply": { "model": "ropebridge:bridge_block_2_jungle" }},
        {"when": { "type": "jungle", "rotated": "true" }, "apply": { "model": "ropebridge:bridge_block_2_jungle", "y": 90 }},
        {"when": { "type": "acacia", "rotated": "false" }, "apply": { "model": "ropebridge:bridge_block_2_acacia" }},
        {"when": { "type": "acacia", "rotated": "true" }, "apply": { "model": "ropebridge:bridge_block_2_acacia", "y": 90 }},
        {"when": { "type": "big_oak", "rotated": "false" }, "apply": { "model": "ropebridge:bridge_block_2_big_oak" }},
        {"when": { "type": "big_oak", "rotated": "true" }, "apply": { "model": "ropebridge:bridge_block_2_big_oak", "y": 90 }},

        {"when": { "OR" : [
            { "front": "1", "rotated": "false" },
            { "front": "2", "rotated": "false" },
            { "front": "3", "rotated": "false" }
        ] }, "apply": { "model": "ropebridge:bridge_attach_2" }},
        {"when": { "OR" : [
            { "front": "2", "rotated": "false" },
            { "front": "3", "rotated": "false" }
        ] }, "apply": { "model": "ropebridge:bridge_attach_3" }},
        {"when": { "front": "3", "rotated": "false" }, "apply": { "model": "ropebridge:bridge_attach_4" }},

        {"when": { "OR" : [
            { "front": "1", "rotated": "true" },
            { "front": "2", "rotated": "true" },
            { "front": "3", "rotated": "true" }
        ] }, "apply": { "model": "ropebridge:bridge_attach_2", "y": 90 }},
        {"when": { "OR" : [
            { "front": "2", "rotated": "true" },
            { "front": "3", "rotated": "true" }
        ] }, "apply": { "model": "ropebridge:bridge_attach_3", "y": 90 }},
        {"when": { "front": "3", "rotated": "true" }, "apply": { "model": "ropebridge:bridge_attach_4", "y": 90 }},

        {"when": { "OR" : [
            { "back": "1", "rotated": "false" },
            { "back": "2", "rotated": "false" },
            { "back": "3", "rotated": "false" }
        ] }, "apply": { "model": "ropebridge:bridge_attach_2", "y": 180 }},
        {"when": { "OR" : [
            { "back": "2", "rotated": "false" },
            { "back": "3", "rotated": "false" }
        ] }, "apply": { "model": "ropebridge:bridge_attach_3", "y": 180 }},
        {"when": { "back": "3", "rotated": "false" }, "apply": { "model": "ropebridge:bridge_attach_4", "y": 180 }},

        {"when": { "OR" : [
            { "back": "1", "rotated": "true" },
            { "back": "2", "rotated": "true" },
            { "back": "3", "rotated": "true" }
        ] }, "apply": { "model": "ropebridge:bridge_attach_2", "y": 270 }},
        {"when": { "OR" : [
            { "back": "2", "rotated": "true" },
            { "back": "3", "rotated": "true" }
        ] }, "apply": { "model": "ropebridge:bridge_attach_3", "y": 270 }},
        {"when": { "back": "3", "rotated": "true" }, "apply": { "model": "ropebridge:bridge_attach_4", "y": 270 }}
    ]
}
